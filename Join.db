CREATE TABLE employee (                                             # Create employee table
    emp_id SERIAL PRIMARY KEY,
    name VARCHAR(50),
    dep_id INT
);

CREATE TABLE department (                                           # Create department table
    dep_id INT PRIMARY KEY,
    dep_name VARCHAR(50)
);

